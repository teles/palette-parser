{
  "name": "palette-parser",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:test": "vue-tsc -b && vite build --mode test",
    "preview": "vite preview",
    "commit": "cz",
    "test": "vitest",
    "test:ci": "vitest run --coverage --silent",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier --write 'src/**/*.{js,ts,vue,json,css,scss,md}'"
  },
  "dependencies": {
    "1-liners": "^0.4.2",
    "@primevue/themes": "^4.0.6",
    "@types/node": "^22.5.3",
    "class-variance-authority": "^0.7.0",
    "detect-incognito": "^1.0.0",
    "dompurify": "^3.1.7",
    "markdown-it": "^14.1.0",
    "pinia": "^2.2.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.0.6",
    "ramda": "^0.30.1",
    "tailwindcss-primeui": "^0.3.4",
    "vue": "^3.4.37",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@pinia/testing": "^0.1.5",
    "@storybook/addon-essentials": "^8.3.4",
    "@storybook/addon-interactions": "^8.3.4",
    "@storybook/addon-links": "^8.3.4",
    "@storybook/addon-onboarding": "^8.3.4",
    "@storybook/blocks": "^8.3.4",
    "@storybook/test": "^8.3.4",
    "@storybook/vue3": "^8.3.4",
    "@storybook/vue3-vite": "^8.3.4",
    "@types/dompurify": "^3.0.5",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/markdown-it": "^14.1.2",
    "@types/ramda": "^0.30.2",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitest/coverage-v8": "^2.1.1",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.2.1",
    "esbuild": "0.24.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.9.0",
    "eslint-plugin-vue": "^9.28.0",
    "git-cz": "^4.9.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "sass": "^1.79.3",
    "storybook": "^8.3.4",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.1",
    "vite-plugin-static-copy": "^1.0.6",
    "vitest": "^2.1.1",
    "vue-tsc": "^2.0.29"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz-config.cjs"
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
